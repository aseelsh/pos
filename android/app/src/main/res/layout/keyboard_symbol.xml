<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
	 xmlns:android="http://schemas.android.com/apk/res/android"
	 xmlns:tools="http://schemas.android.com/tools"
	 xmlns:custom="http://schemas.android.com/apk/res-auto"
	 android:layout_width="match_parent"
	 android:layout_height="match_parent"
	 android:background="@color/dk_gray"
	 android:orientation="vertical">

  <LinearLayout
		android:layout_height="0dp"
		android:layout_width="match_parent"
		android:layout_weight="1"
		android:orientation="horizontal">
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_1"
		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="1"
		  android:textSize="@dimen/keyboard_text"/>
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_2"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="2"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_3"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="3"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_4"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="4"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_5"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="5"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_6"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="6"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_7"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="7"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_8"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="8"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_9"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="9"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_0"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="0"
		  android:textSize="@dimen/keyboard_text"/>

  </LinearLayout>
  
  <LinearLayout
		android:layout_height="0dp"
		android:layout_width="match_parent"
		android:layout_weight="1"
		android:orientation="horizontal">
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_at"
		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="\@"
		  android:textSize="@dimen/keyboard_text"/>
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_hash"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="#"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_dollar"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="$"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_percent"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="%"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_amp"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="&amp;"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_minus"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="-"
		  android:textSize="@dimen/keyboard_text"/>
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_plus"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="+"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_l_paren"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="("
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_r_paren"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text=")"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosIconButton
		  android:id="@+id/keyboard_del"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="@string/fa_del"
		  android:textSize="@dimen/keyboard_text"/>

  </LinearLayout>
  
  <LinearLayout
		android:layout_height="0dp"
		android:layout_width="match_parent"
		android:layout_weight="1"
		android:orientation="horizontal">
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_back_slash"
		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="\\\"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_equal"
		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="="
		  android:textSize="@dimen/keyboard_text"/>
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_asterisk"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="*"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_d_quote"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="&quot;"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_s_quote"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="&apos;"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_colon"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text=":"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_semicolon"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text=";"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_exclaim"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="!"
		  android:textSize="@dimen/keyboard_text"/>
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_question"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="?"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosIconButton
		  android:id="@+id/keyboard_next"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="@string/fa_arrow_right"
		  android:textSize="@dimen/keyboard_text"/>

  </LinearLayout>
  
  <LinearLayout
		android:layout_height="0dp"
		android:layout_width="match_parent"
		android:layout_weight="1"
		android:orientation="horizontal">
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_toggle"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="2"
		  android:text="ABC"
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_comma"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text=","
		  android:textSize="@dimen/keyboard_text"/>

	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_space"
		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="5"
		  android:text="SPACE"
		  android:textSize="@dimen/keyboard_text"/>
	 
	 <cloud.multipos.pos.views.PosButton
		  android:id="@+id/keyboard_period"
 		  style="@style/LoginButton"
		  android:layout_margin="@dimen/keyboard_margin"
		  android:layout_height="match_parent"
		  android:layout_width="0dp"
		  android:layout_weight="1"
		  android:text="."
		  android:textSize="@dimen/keyboard_text"/>

  </LinearLayout>  <!-- row end -->
  
</LinearLayout>  <!-- keyboard end -->
