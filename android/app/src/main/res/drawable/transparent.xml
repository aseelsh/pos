<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <gradient
          android:startColor="#44eeeeee"
          android:endColor="#44dddddd"
          android:angle="90" />
      <stroke
          android:width="2dp"
          android:color="#ffffff" />
      <corners
          android:radius="3dp" />

    </shape>
  </item>    
  <item>
    <shape>
      <gradient
          android:startColor="#44eeeeee"
          android:endColor="#44dddddd"
          android:angle="90" />
      <stroke
          android:width="2dp"
          android:color="#33ffffff" />
      <corners
          android:radius="3dp" />

    </shape>
  </item>
</selector>
